			<td>USERNAME</td>
			<td>Email</td>
			<td>Phone</td>
			<td>Blood Group</td>
			<td>status</td>
		</tr>
		<% for(var i=0; i < userlist.length; i++){ %>
		<tr>
			<td><%= userlist[i].fname %></td>
			<td><%= userlist[i].lname %></td>
			<td><%= userlist[i].username %></td>
			<td><%= userlist[i].email %></td>
			<td><%= userlist[i].phone %></td>
			<td><%= userlist[i].bloodgroup%></td>
			
			<td><%= userlist[i].status %></td>
			<td>
				<a href="/admin/view_Patient/<%= userlist[i].username %>">View Profile </a>
			</td>
		</tr>
		<%  } %>
	</table>

</body>

</html>